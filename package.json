    "supertest": "^1.0.1",
    "uglify-loader": "^1.2.0",
    "webpack": "^1.10.5"
  },
  "scripts": {
    "lint": "gulp lint",
    "dev": "DEBUG=*,-babel,-express:*,-mquery,-pool2,-knex:client,-eslint:*,-tinylr:* gulp dev",
    "migrate": "DEBUG=*,-babel node init.js migrate",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --timeout 50000 --reporter dot test/**/**",
    "server": "node init.js"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "forOf": true,
      "defaultParams": true,
      "jsx": true,
      "modules": true
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "mocha": true,
      "jasmine": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true
    },
    "rules": {
      "strict": 2,
      "eqeqeq": [
        2,
        "smart"
      ],
      "no-eval": 1,
      "indent": [
        2,
        2,
        {
          "indentSwitchCase": true
        }
      ],
      "no-underscore-dangle": 0,
      "no-unused-vars": [
        1,
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "no-undef": 0,
      "semi": [
        2,
        "always"
      ],
      "max-statements": [
        1,
        30
      ],
      "max-depth": [
        1,
        3
      ],
      "complexity": [
        1,
        7
      ]
    }
  }
}
