  "devDependencies": {
    "babel-core": "5.8.25",
    "babel-eslint": "4.1.3",
    "babel-loader": "5.3.2",
    "babel-runtime": "5.8.25",
    "del": "2.0.2",
    "eslint": "1.7.1",
    "eslint-plugin-react": "3.5.1",
    "gulp": "3.9.0",
    "gulp-apidoc": "0.2.0",
    "gulp-babel": "5.3.0",
    "gulp-batch": "1.0.5",
    "gulp-concat": "2.6.0",
    "gulp-eslint": "1.0.0",
    "gulp-livereload": "3.8.1",
    "gulp-load-plugins": "1.0.0",
    "gulp-minify-css": "1.2.1",
    "gulp-nodemon": "2.0.4",
    "gulp-rename": "1.2.2",
    "gulp-sass": "2.0.4",
    "gulp-size": "1.2.3",
    "gulp-sourcemaps": "1.5.2",
    "gulp-uglify": "1.2.0",
    "gulp-util": "3.0.6",
    "mocha": "2.2.5",
    "nodemon": "1.7.1",
    "should": "6.0.3",
    "source-map-support": "0.3.2",
    "supertest": "1.0.1",
    "webpack": "1.12.2"
  },
  "scripts": {
    "lint": "gulp lint",
    "dev": "DEBUG=*,-minilr,-minilr:*,-nodemon,-nodemon:*,-babel,-express:*,-mquery,-pool2,-knex:client,-eslint:*,-tinylr:*,-send,-body-parser:* gulp dev",
    "dev-admin": "PROJECT_NAME=admin npm run dev",
    "dev-api": "PROJECT_NAME=api npm run dev",
    "build-site": "PROJECT_NAME=site NODE_ENV=production gulp build-clients",
    "build-admin": "PROJECT_NAME=admin NODE_ENV=production gulp build-clients",
    "clean-dist": "gulp clean-dist",
    "distribute-site": "PROJECT_NAME=site NODE_ENV=production gulp dist-clients",
    "distribute-admin": "PROJECT_NAME=admin NODE_ENV=production gulp dist-clients",
    "migrate": "DEBUG=*,-babel node init.js migrate",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --timeout 50000 --reporter dot test/**/**",
    "apidoc": "gulp apidoc",
    "server": "node init.js"
  },
  "eslintConfig": {
    "ecmaFeatures": {
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "forOf": true,
      "defaultParams": true,
      "jsx": true,
      "modules": true,
      "superInFunctions": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "mocha": true,
      "jasmine": true
    },
    "settings": {
      "ecmascript": 6,
      "jsx": true
    },
    "rules": {
      "strict": 2,
      "eqeqeq": [
        2,
        "smart"
      ],
      "no-eval": 1,
      "indent": [
        2,
        2,
        {
          "SwitchCase": 1
        }
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "no-underscore-dangle": 0,
      "no-unused-vars": [
        1,
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "no-undef": 0,
      "semi": [
        2,
        "always"
      ],
      "max-statements": [
        1,
        30
      ],
      "max-depth": [
        1,
        3
      ],
      "complexity": [
        1,
        10
      ],
      "no-labels": 0,
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "react/display-name": [
        1,
        {
          "acceptTranspilerName": true
        }
      ],
      "react/jsx-boolean-value": 1,
      "react/jsx-curly-spacing": 1,
      "react/jsx-no-duplicate-props": 1,
      "react/jsx-no-undef": 1,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "react/no-danger": 1,
      "react/no-did-mount-set-state": 1,
      "react/no-did-update-set-state": 1,
      "react/no-multi-comp": 1,
      "react/no-unknown-property": 1,
      "react/prop-types": 1,
      "react/react-in-jsx-scope": 1,
      "react/require-extension": 1,
      "react/self-closing-comp": 1,
      "react/sort-comp": 1,
      "react/wrap-multilines": 1
    }
  },
  "apidoc": {
    "name": "Homehapp Mobile Backend",
    "title": "Homehapp Mobile Backend",
    "description": "Homehapp Mobile Backend documentation",
    "version": "0.1.0",
    "order": [
      "Authentication",
      "Homes"
    ],
    "template": {
      "withGenerator": false
    }
  }
}
