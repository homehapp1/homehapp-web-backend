    "bluebird": "2.10.2",
    "body-parser": "1.14.1",
    "classnames": "2.1.5",
    "cloudinary": "1.2.5",
    "connect": "3.4.0",
    "connect-mongodb": "1.1.5",
    "cookie-parser": "1.4.0",
    "csurf": "1.8.3",
    "debug": "2.2.0",
    "ejs": "2.3.4",
    "express": "4.13.3",
    "express-layout": "0.1.0",
    "express-partials": "0.3.0",
    "express-session": "1.11.3",
    "express-winston": "0.4.1",
    "html-entities": "1.2.0",
    "iso": "4.2.0",
    "jsonwebtoken": "5.4.0",
    "linear-partition": "0.0.2",
    "moment": "2.10.6",
    "moment-timezone": "0.4.1",
    "mongoose": "4.1.11",
    "passport": "0.3.0",
    "passport-local": "1.0.0",
    "passport-strategy": "1.0.0",
    "react": "0.13.3",
    "react-bootstrap": "0.24.5",
    "react-cookie": "^0.4.1",
    "react-google-maps": "2.2.0",
    "react-router": "0.13.4",
    "react-router-bootstrap": "0.18.1",
    "serve-favicon": "2.2.1",
    "twilio": "2.5.1",
    "uuid": "2.0.1",
    "winston": "1.1.0"
